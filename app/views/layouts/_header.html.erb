<header class="navbar navbar-fixed-top navbar-inverse">
  <div class="container">

    <nav>
      <ul class="nav navbar-nav navbar-right">
        <li><%= link_to "About", '#' %></li>
        <% if logged_in? %>
          <li><%= link_to "Log out", logout_path, method: :delete %></li>
          <li><%= link_to "Settings", edit_user_path(current_user) %></li>
        <% else %>
          <li><%= link_to "Log in", login_path %></li>
        <% end %>
      </ul>
    </nav>

    <header class="header">
      <small>
        <%= link_to image_tag("Instacart_logo_header.png", alt: "instacart logo", class:"header-image-container"), 'http://www.instacart.com/' %>
      </small>
    </header>

  </div>
</header>
